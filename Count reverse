class Solution {
    public int countRevPairs(int N, int arr[]) {
        // Code here
        int i=0,t=0;
        while(i<=(N-2)){
            for(int j=i+1;j<N;j++){
                if(arr[i]>(2*arr[j])){
                    t++;
                }
            }i++;
        }
        return t;
    }
}
