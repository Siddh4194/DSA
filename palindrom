int isPalindrome(String S) {
        // code here
            String str1=S.replaceAll("[^a-zA-Z0-9]", "");
    str1=str1.toLowerCase();
    String reverse="";
    for(int i=0;i<str1.length();i++)
    {
        reverse=str1.charAt(i)+reverse;
    }
    if(str1.equals(reverse))
    {
        return 1;
    }
    return 0;
    }
